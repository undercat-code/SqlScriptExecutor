<?xml version="1.0" encoding="utf-8" ?>

<configuration>
	<configSections>
		<section name="appConfig" type="System.Configuration.AppSettingsSection, System.Configuration"></section>
		<section name="emailSenderConfig" type="System.Configuration.AppSettingsSection, System.Configuration"></section>
	</configSections>
	<appConfig>
		<add key="logFilePath" value="path"/>
		<add key ="dbKey" value="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=TestDB;Integrated Security=True"/>
		<add key="scriptFolderPath" value="path"/>
	</appConfig>
	<emailSenderConfig>
		<add key="senderEmailLogin" value="mail.com"/>
		<add key="senderEmailSMTPServer" value="smtp"/>
		<add key="senderEmailPort" value="465"/>
		<add key="senderEmailPassword" value="pass"/>
		<add key="defaultRecipients" value="mail.com"/>
	</emailSenderConfig>
</configuration>
